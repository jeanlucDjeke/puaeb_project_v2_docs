	`coef_fact2` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_math` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_meca` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_tprat` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_pc` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_frEcrit` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_frOral` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_philo` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_angOral` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_eps` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_fact1` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`barem_fact2` CHAR(50) NULL DEFAULT NULL COLLATE 'utf8_unicode_ci',
	`math` FLOAT NULL DEFAULT NULL,
	`meca` FLOAT NULL DEFAULT NULL,
	`t_prat` FLOAT NULL DEFAULT NULL,
	`pc` FLOAT NULL DEFAULT NULL,
	`frEcrit` FLOAT NULL DEFAULT NULL,
	`frOral` FLOAT NULL DEFAULT NULL,
	`philo` FLOAT NULL DEFAULT NULL,
	`ang_Oral` FLOAT NULL DEFAULT NULL,
	`eps` FLOAT NULL DEFAULT NULL,
	`fact1` FLOAT NULL DEFAULT NULL,
	`fact2` FLOAT NULL DEFAULT NULL,
	`id_etablissement` INT(11) NULL DEFAULT NULL,
	`id_type_resultat` INT(11) NULL DEFAULT NULL,
	`id_dren` INT(11) NULL DEFAULT NULL,
	`id_statut` INT(11) NULL DEFAULT NULL,
	`id_annee` INT(11) NULL DEFAULT NULL,
	`id_sexe` INT(11) NULL DEFAULT NULL,
	PRIMARY KEY (`ID`),
	INDEX `id_dren` (`id_dren`),
	INDEX `id_etablissement` (`id_etablissement`),
	INDEX `id_statut` (`id_statut`),
	INDEX `id_annee` (`id_annee`),
	INDEX `id_sexe` (`id_sexe`),
	INDEX `id_type_resultat` (`id_type_resultat`),
	CONSTRAINT `fait_exam_bac_e_ibfk_1` FOREIGN KEY (`id_dren`) REFERENCES `dim_dren` (`ID`),
	CONSTRAINT `fait_exam_bac_e_ibfk_2` FOREIGN KEY (`id_etablissement`) REFERENCES `dim_etablissement` (`ID`),
	CONSTRAINT `fait_exam_bac_e_ibfk_3` FOREIGN KEY (`id_statut`) REFERENCES `dim_statut` (`ID`),
	CONSTRAINT `fait_exam_bac_e_ibfk_4` FOREIGN KEY (`id_annee`) REFERENCES `dim_annee_scolaire` (`ID`),
	CONSTRAINT `fait_exam_bac_e_ibfk_5` FOREIGN KEY (`id_sexe`) REFERENCES `dim_sexe` (`ID`),
	CONSTRAINT `fait_exam_bac_e_ibfk_6` FOREIGN KEY (`id_type_resultat`) REFERENCES `dim_type_resultat` (`ID`)
)
COLLATE='utf8_unicode_ci';


